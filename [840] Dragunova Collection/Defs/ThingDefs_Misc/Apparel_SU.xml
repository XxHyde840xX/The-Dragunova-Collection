	<texPath>Things/Apparel/SU_Defisa</texPath>
    <graphicClass>Graphic_Single</graphicClass> 
	 <drawSize>0.90</drawSize>
	</graphicData>
    <thingCategories>
      <li>SU_Apparels</li>
    </thingCategories>
