<Defs>

  <!--=================================== AP Cartridges ==========================================-->

  <RecipeDef>
    <defName>Make_SU_AP</defName>
    <label>Craft AP Cartridges</label>
    <description>Make AP Cartridges.</description>
    <jobString>Making AP Cartridges.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>5000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>125</count>
      </li>
    </ingredients>
    <products>
      <SU_ContainerAP>75</SU_ContainerAP>
    </products>
    <skillRequirements>
      <Crafting>10</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
	<researchPrerequisite>IlyaTech</researchPrerequisite>
  </RecipeDef>
  
